    <style name="FabMenuButtonSmallStyle">
        <item name="fab_size">mini</item>
        <item name="fab_colorNormal">?attr/fabDefaultColorNormal</item>
        <item name="fab_colorPressed">?attr/fabDefaultColorPressed</item>
        <item name="fab_colorRipple">?attr/fabDefaultColorRipple</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
