<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="FabMenuStyle">
        <item name="fab_size">mini</item>
        <item name="menu_colorNormal">?attr/fabDefaultColorNormal</item>
        <item name="menu_colorPressed">?attr/fabDefaultColorPressed</item>
        <item name="menu_colorRipple">?attr/fabDefaultColorRipple</item>
        <item name="menu_animationDelayPerItem">0</item>
        <item name="menu_labels_maxLines">2</item>
        <item name="menu_labels_ellipsize">end</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="FabMenuLabelsStyle">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">14sp</item>
        <item name="android:maxLines">2</item>
        <item name="android:ellipsize">end</item>
    </style>

